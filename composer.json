{
    "name": "workup/advanced-nova-media-library",
    "description": "Laravel Nova tools for managing the Spatie media library.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^8.0|^9.0",
        "workup/nova": "^3.0",
        "spatie/laravel-medialibrary": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Workup\\AdvancedNovaMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workup\\AdvancedNovaMediaLibrary\\AdvancedNovaMediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
